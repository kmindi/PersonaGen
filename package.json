{
    "name": "persona-gen",
    "description": "Generate software developer personas",
    "version": "0.0.1",
    "author": "Justus Bogner <justus.bogner@gmail.com>",
    "private": true,
    "scripts": {
        "build": "webpack --config webpack.prod.config.js --json --progress | tail -n +2 > ./build.log.json",
        "start": "webpack-dev-server --open --hot --config webpack.dev.config.js",
        "start-prod": "webpack-dev-server --open --config webpack.prod.config.js",
        "lint": "tslint -c tslint.json src/**/*.ts",
        "test": "jest"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "js",
            "json",
            "vue"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "transform": {
            ".*\\.(vue)$": "vue-jest",
            "^.+\\.tsx?$": "ts-jest"
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{ts,vue}",
            "!**/node_modules/**"
        ],
        "coverageReporters": [
            "html",
            "text-summary"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.8",
        "@fortawesome/fontawesome-free-brands": "^5.0.13",
        "@fortawesome/fontawesome-free-regular": "^5.0.13",
        "@fortawesome/fontawesome-free-solid": "^5.0.13",
        "@fortawesome/vue-fontawesome": "^0.0.22",
        "@panter/vue-i18next": "^0.10.1",
        "@types/i18next": "^8.4.3",
        "axios": "^0.18.0",
        "bootstrap": "^4.1.1",
        "bootstrap-vue": "^2.0.0-rc.11",
        "bootswatch": "^4.1.1",
        "core-js": "^2.5.7",
        "file-saver": "^1.3.8",
        "i18next": "^11.3.2",
        "moment": "^2.22.1",
        "vue": "^2.5.16",
        "vue-class-component": "^6.2.0",
        "vue-property-decorator": "^6.1.0",
        "vue-router": "^3.0.1"
    },
    "devDependencies": {
        "@types/file-saver": "^1.3.0",
        "@types/jest": "^22.2.3",
        "@vue/test-utils": "^1.0.0-beta.16",
        "cname-webpack-plugin": "^1.0.2",
        "copy-webpack-plugin": "^4.5.1",
        "css-loader": "^0.28.11",
        "file-loader": "^1.1.11",
        "git-revision-webpack-plugin": "^2.5.1",
        "jest": "^23.0.1",
        "json-loader": "^0.5.7",
        "less": "^3.0.4",
        "less-loader": "^4.1.0",
        "script-loader": "^0.7.2",
        "string-replace-loader": "^2.1.1",
        "style-loader": "^0.21.0",
        "ts-jest": "^22.4.6",
        "ts-loader": "^3.5.0",
        "tslint": "^5.10.0",
        "tslint-loader": "^3.6.0",
        "tslint-webpack-plugin": "^1.2.2",
        "typescript": "^2.8.3",
        "url-loader": "^1.0.1",
        "vue-jest": "^2.6.0",
        "vue-loader": "^14.2.2",
        "vue-template-compiler": "^2.5.16",
        "webpack": "^3.11.0",
        "webpack-dev-server": "^2.11.2",
        "webpack-file-changer": "^2.0.1",
        "webpack-merge": "^4.1.2"
    }
}
